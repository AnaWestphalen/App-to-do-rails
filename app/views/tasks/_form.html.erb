<%= simple_form_for [@list, @task] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :completed %>
  </div>

  <div class="form-actions">
     <%= f.submit "Adicionar a tarefa", class: "btn btn-primary" %>
  </div>
<% end %>
